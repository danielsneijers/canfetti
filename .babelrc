{
  "presets": ["@babel/preset-env"],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": true }]],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": { "browsers": ["last 2 versions", "> 5%"] }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "browsers": ["last 2 versions", "> 5%"] } }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          { "targets": { "browsers": ["last 2 versions", "> 5%"] } }
        ]
      ]
    }
  }
}
